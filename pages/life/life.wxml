<!--daren.wxml-->
<include src="../header/header.wxml"/>
<view class="container">
  <view class='yy'>
    <view class='yy_nav'>
      <rich-text class='htz' catchtap="showhistory">发展历程</rich-text>
      <rich-text class='htz yy_nav_se'>{{classname}}</rich-text>
      <rich-text class='htz' catchtap="showselection">丰雷先生评选</rich-text>
    </view>
    <text>{{classname}}</text>
   <view>
		<import src="../../wxParse/wxParse.wxml"/>
		<template is="wxParse" data="{{wxParseData:content.nodes}}"/>
	</view>
  </view>
  <view class='daren'>
    <view class='daren_01' wx:for="{{list}}" wx:key="{{item.id}}">
      <video src='{{item.vurl}}' poster='{{item.picurl}}' ></video>
    </view>
  </view>
</view>
<include src="../footer/footer.wxml"/>