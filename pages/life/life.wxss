/**product.wxss**/
.daren{
  padding: 10px;
  margin-bottom: 15px;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  }
.daren .daren_01{
  width: calc(50% - 10px);
  float: left;
  margin: 5px;
  }
.daren .daren_01 video{
  width:100%;
  display: block;
  }
.daren .daren_01 text{
  display: block;
  background: #e9e9e9;
  color: #333;
  font-size: 14px;
  text-align: center;
  line-height: 20px;
  padding: 10px;
}